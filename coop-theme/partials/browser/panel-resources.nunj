{% set compHandle = entity.component().handle %}
{% for collection in entity.resources().items() %}
{% if collection.size %}
{% for resource in collection.items() %}
    {% if resource.name == 'supporting' %}
    <div class="row">
        <div class="column medium-10 small-12">
            <section class="ds-section">
                {{ resource.contents }}    
            </section>
        </div>
    </div>
    {% endif %}
{% endfor %}
{% endif %}
{% endfor %}
